{
  "name": "fresh-manager",
  "version": "1.0.0",
  "description": "赞播优鲜-管理",
  "author": "Ben <xuedengheng@outlook.com>",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vue-cli-service serve --mode development",
    "release": "vue-cli-service serve --mode release",
    "prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build --modern",
    "build:dev": "vue-cli-service build --mode development",
    "build:release": "vue-cli-service build --mode release",
    "build:prod": "vue-cli-service build --mode production",
    "build:ci": "vue-cli-service build --report",
    "lint": "run-s lint:all:*",
    "lint:all:prettier": "prettier --write --loglevel warn \"**/*.{js,json,css,vue}\"",
    "lint:all:eslint": "eslint --fix --ext .js,.vue .",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:file": "npm run test:unit --bail --findRelatedTests",
    "test:unit:ci": "npm run test:unit --coverage --ci",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test": "run-s test:unit test:e2e",
    "test:ci": "run-s test:unit:ci test:e2e",
    "new": "hygen new"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "echarts": "^4.2.0-rc.2",
    "element-ui": "^2.6.3",
    "iview": "^3.1.5",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "print-js": "^1.0.57",
    "storage-controller": "^1.0.4",
    "vue": "^2.5.17",
    "vue-meta": "^1.5.6",
    "vue-router": "^3.0.1",
    "vue-slicksort": "^1.1.3",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1",
    "weixin-js-sdk": "^1.4.0-test"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-e2e-cypress": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/eslint-config-standard": "4.0.x",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "5.2.x",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "5.0.0",
    "husky": "^1.2.0",
    "hygen": "1.6.x",
    "imagemin-lint-staged": "0.3.x",
    "lint-staged": "8.0.x",
    "npm-run-all": "4.1.x",
    "qr-image": "^3.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cropperjs": "^2.2.2",
    "vue-jest": "2.6.x",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
